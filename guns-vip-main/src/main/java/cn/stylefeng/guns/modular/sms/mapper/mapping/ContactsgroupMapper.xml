<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.stylefeng.guns.modular.sms.mapper.ContactsgroupMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.stylefeng.guns.modular.sms.entity.Contactsgroup">
        <id column="groupid" property="groupid" />
        <result column="groupname" property="groupname" />
        <result column="orderid" property="orderid" />
        <result column="entid" property="entid" />
        <result column="remark" property="remark" />
        <result column="adddate" property="adddate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        groupid AS "groupid", groupname AS "groupname", orderid AS "orderid", entid AS "entid", remark AS "remark", adddate AS "adddate"
    </sql>


    <select id="customList" resultType="cn.stylefeng.guns.modular.sms.model.result.ContactsgroupResult" parameterType="cn.stylefeng.guns.modular.sms.model.params.ContactsgroupParam">
        select
        <include refid="Base_Column_List"/>
        from contactsgroup where 1 = 1
            <if test="paramCondition.groupname != null and paramCondition.groupname != ''">
                and groupname like CONCAT('%',#{paramCondition.groupname},'%')
            </if>
        <if test="paramCondition.entid != null and paramCondition.entid != ''">
            and entid like CONCAT('%',#{paramCondition.entid},'%')
        </if>
    </select>

    <select id="customMapList" resultType="map" parameterType="cn.stylefeng.guns.modular.sms.model.params.ContactsgroupParam">
        select
        <include refid="Base_Column_List"/>
        from contactsgroup where 1 = 1
            <if test="paramCondition.groupname != null and paramCondition.groupname != ''">
                and groupname like CONCAT('%',#{paramCondition.groupname},'%')
            </if>
        <if test="paramCondition.entid != null and paramCondition.entid != ''">
            and entid like CONCAT('%',#{paramCondition.entid},'%')
        </if>
    </select>

    <select id="customPageList" resultType="cn.stylefeng.guns.modular.sms.model.result.ContactsgroupResult" parameterType="cn.stylefeng.guns.modular.sms.model.params.ContactsgroupParam">
        select
        <include refid="Base_Column_List"/>
        from contactsgroup where 1 = 1
            <if test="paramCondition.groupname != null and paramCondition.groupname != ''">
                and groupname like CONCAT('%',#{paramCondition.groupname},'%')
            </if>
        <if test="paramCondition.entid != null and paramCondition.entid != ''">
            and entid like CONCAT('%',#{paramCondition.entid},'%')
        </if>
    </select>

    <select id="customPageMapList" resultType="map" parameterType="cn.stylefeng.guns.modular.sms.model.params.ContactsgroupParam">
        select
        <include refid="Base_Column_List"/>
        from contactsgroup where 1 = 1
            <if test="paramCondition.groupname != null and paramCondition.groupname != ''">
                and groupname like CONCAT('%',#{paramCondition.groupname},'%')
            </if>
        <if test="paramCondition.entid != null and paramCondition.entid != ''">
            and entid like CONCAT('%',#{paramCondition.entid},'%')
        </if>
    </select>

</mapper>
